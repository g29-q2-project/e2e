						script(type='text/javascript', src='/js/plugins/jquery-1.11.2.min.js')
						// materialize js
						script(type='text/javascript', src='/js/materialize.js')
						// chartist
						script(type='text/javascript', src='/js/plugins/chartist-js/chartist.min.js')
						// chartjs
						script(type='text/javascript', src='/js/plugins/chartjs/chart.min.js')
						script(type='text/javascript', src='/js/plugins/chartjs/chart-script.js')
						// sparkline
						script(type='text/javascript', src='/js/plugins/sparkline/jquery.sparkline.min.js')
						script(type='text/javascript', src='/js/plugins/sparkline/sparkline-script.js')
