doctype html
html(lang='en')
	head
		meta(http-equiv='Content-Type', content='text/html; charset=UTF-8')
		meta(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no')
		meta(http-equiv='X-UA-Compatible', content='IE=edge')
		meta(name='msapplication-tap-highlight', content='no')
		meta(name='description', content="Materialize is a Material Design Admin Template,It's modern, responsive and based on Material Design by Google. ")
		meta(name='keywords', content='materialize, admin template, dashboard template, flat admin template, responsive admin template,')
		title e2e Dashboard

		link(href='/css/materialize.css', type='text/css', rel='stylesheet', media='screen,projection')
		link(href='/css/style.css', type='text/css', rel='stylesheet', media='screen,projection')
		// Custome CSS
		link(href='/css/custom/custom.css', type='text/css', rel='stylesheet', media='screen,projection')
		// INCLUDED PLUGIN CSS ON THIS PAGE
		link(href='/js/plugins/jvectormap/jquery-jvectormap.css', type='text/css', rel='stylesheet', media='screen,projection')
		link(href='/js/plugins/chartist-js/chartist.min.css', type='text/css', rel='stylesheet', media='screen,projection')
	body
		// Start Page Loading
		#loader-wrapper
			#loader
			.loader-section.section-left
			.loader-section.section-right
		// End Page Loading
		// ////////////////////////////////////////////////////////////////////////////
		// START HEADER
		header#header.page-topbar
			// start header nav
			.navbar-fixed
				nav.primary-color
					.nav-wrapper
						ul.left
							li
								h1.logo-wrapper
									a.brand-logo.darken-1(href='index.html')
										img(src='/images/materialize-logo.png', alt='materialize logo')
									span.logo-text Materialize
						ul.right.hide-on-med-and-down
							li
								a.waves-effect.waves-block.waves-light.toggle-fullscreen(href='javascript:void(0);')
									i.mdi-action-settings-overscan
							li
								a.waves-effect.waves-block.waves-light.toggle-fullscreen(href='dashTest.html')
									i.mdi-action-dashboard
							li
								a.waves-effect.waves-block.waves-light.toggle-fullscreen(href='profileTest.html')
									i.mdi-action-face-unlock
							li
								a.waves-effect.waves-block.waves-light.notification-button(href='javascript:void(0);', data-activates='notifications-dropdown')
									i.mdi-av-timer
							li
								a.waves-effect.waves-block.waves-light.chat-collapse(href='#', data-activates='chat-out')
									i.mdi-hardware-keyboard-tab
									// Add Logout Route
									// notifications-dropdown
									ul#notifications-dropdown.dropdown-content
										li
											h5
												| TRIP TIMES
										li.divider
										li
											a(href='#!')
												i.mdi-maps-directions-bike
												|  Commute to Work - 3min longer than average
										li
											a(href='#!')
												i.mdi-action-stars
												|  Commute to Daycare - 5min longer than average
			// end header nav
			// END HEADER
			// ////////////////////////////////////////////////////////////////////////////
			// START MAIN
		#main
			// START WRAPPER
			.wrapper
				// START LEFT SIDEBAR NAV
				aside#left-sidebar-nav
					ul#slide-out.side-nav.fixed.leftside-navigation
						li.user-details.cyan
							.row
								.col.col.s4.m4.l4
									img.circle.responsive-img.valign.profile-image(src='/images/avatar.jpg', alt='')
								.col.col.s8.m8.l8
									ul#profile-dropdown.dropdown-content
										li
											a(href='#')
												i.mdi-action-face-unlock
												|  Profile
										li
											a(href='#')
												i.mdi-action-settings
												|  Settings
										li
											a(href='#')
												i.mdi-communication-live-help
												|  Help
										li.divider
										li
											a(href='#')
												i.mdi-action-lock-outline
												|  Lock
										li
											a(href='#')
												i.mdi-hardware-keyboard-tab
												|  Logout
									a.btn-flat.dropdown-button.waves-effect.waves-light.white-text.profile-btn(href='#', data-activates='profile-dropdown')
										| Tim Pender
										i.mdi-navigation-arrow-drop-down.right
									p.user-roal Administrator
						li.bold
							a.waves-effect.waves-cyan(href='dashboard/index.html')
								i.mdi-action-dashboard
								|  Dashboard
						li.bold
							a.waves-effect.waves-cyan(href='/profileTest')
								i.mdi-action-face-unlock
								|  Profile
						li.bold
							a.waves-effect.waves-cyan(href='#')
								i.mdi-communication-email
								|  Contact Us
					a.sidebar-collapse.btn-floating.btn-medium.waves-effect.waves-light.hide-on-large-only.cyan(href='#', data-activates='slide-out')
						i.mdi-navigation-menu
					// END LEFT SIDEBAR NAV
					// ////////////////////////////////////////////////////////////////////////////
					// START CONTENT
				section#content
					// start container
					.container
						.row.center-align
							.col.s12.m5.l5
								.card
									.card-move-up.teal.waves-effect.waves-block.waves-light
										.move-up
											h5.margin.white-text Start Location
											input#origin-input.controls(type='text', name='trip_name', placeholder='starting location for your trip')
							.col.s12.m5.l5
								.card
									.card-move-up.teal.waves-effect.waves-block.waves-light
										.move-up
											h5.margin.white-text Destination Location
											input#destination-input.controls(type='text', name='trip_name', placeholder='starting location for your trip')
							.col.s12.m2.l2.valign-wrapper
								a#getDirections.btn-floating.valign.btn-large.waves-effect.waves-light.cyan
									i.mdi-content-add
					
							#map(style="display:none")
							#jon_map
							#floating-panel
								b Mode of Travel:
								select#mode
									option(value='TRANSIT') Transit
									option(value='BICYCLING') Bicycling
									option(value='DRIVING') Driving
									option(value='WALKING') Walking
								button#showTraffic traffic
								button#showTransit transit
								button#showBike bike
					footer.page-footer
						.container
							.row.section
							.container
								| Copyright Â© 2015
								a.grey-text.text-lighten-4(href='http://themeforest.net/user/geekslabs/portfolio?ref=geekslabs', target='_blank') GeeksLabs
								|  All rights reserved.
								span.right
									| Design and Developed by
									a.grey-text.text-lighten-4(href='http://geekslabs.com/') GeeksLabs
						// END FOOTER
